<import src="/template/navigation/nav.wxml" />
<template is="navigation" data="{{nav:nav}}" />
<view class='bHead' ></view>
<view class='body'>
    <view class='shujia' wx:for="{{booklist}}" wx:for-item="bookd" wx:key="*this" wx:if="{{booklist[0].length>0}}">
    <view class='books {{bookiphone5}}' >
      <view class='book bfinish' wx:key="*thisd" wx:for="{{bookd}}" wx:for-index="idx" wx:for-item="book" style="margin-right:{{idx==3?'0px':'10px'}};" bindtap='gotopage' data-bookid="{{book.id}}">
        <text class='bookname'>{{book.name}}</text>
        <text class='bookauther'>{{book.authorName}}</text>
      </view>
    </view>
  </view>
</view>
